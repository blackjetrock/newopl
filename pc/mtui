cc -g       -Wno-format-truncation -Wno-format-overflow newopl_tran.c \
  newopl_lib.c \
  newopl_types.c \
  parser.c \
  machine.c \
  errors.c \
  -o newopl_tran -lm
cc -g -DTUI -Wno-format-truncation -Wno-format-overflow newopl_exec.c \
  newopl_lib.c \
  newopl_types.c \
  qcode.c \
  time.c \
  clock_pc.c \
  num_a.c \
  machine.c \
  logical_files.c \
  tui.c \
  errors.c \
  svc_pak_linux.c \
  svc_pk.c \
  svc_fl.c \
   svc_kb.c \
   svc_mn.c \
  sysvar.c \
  -o newopl_exec -lcurses -lm
