VERSION:
rem
rem ******** model type *********
rem
local a%
print "Machine: ";
a%=peekb($ffe8)
if a%=0
 print "CM"
elseif a%=1
 print "XP"
elseif a%=2
 print "LA"
elseif a%=13
 print "LZ64"
elseif a%=14
 print "LZ"
else
 print hex$(a%),hex$(peekb($ffcb))
endif
rem
rem ******* version number *********
rem
print "Version: ";
print left$(hex$(peekb($ffe9)),1);
print ".";
print right$(hex$(peekb($ffe9)),1);
rem
rem ******* the end *******
rem 
get