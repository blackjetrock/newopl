QUHUM:
LOCAL G%
CLS
PRINT A$
LOOP::
AT P%,2
PRINT CHR$(94);
AT P%,2
G%=GET
PRINT " ";
IF G%=13
 IF QUMOV:=0
  RETURN -1
 ENDIF
ELSEIF G%=5
 P%=P%-1+(16 AND P%=1)
ELSEIF G%=6
 P%=P%+1-(16 AND P%=16)
ELSEIF G%=1
 PRINT CHR$(15);"Quit? (Y/N)";
 G%=ASC(UPPER$(GET$))
 IF G%=%Y
  RETURN 0
 ENDIF
 PRINT CHR$(15);
ENDIF
GOTO LOOP::
