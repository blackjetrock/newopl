TICINIT:
LOCAL A%,K%

A%=1
DO
 W%(A%)=ASC(MID$("123456789147258369159357",A%,1))-48
 A%=A%+1
UNTIL A%>24

A%=1
DO
 U%(A%)=VAL(MID$("010101310101013101010100000000000101013101010131010101000000000000000028000000280000000000000000",A%+A%-1,2))
 A%=A%+1
UNTIL A%>48

A%=1
DO
 BD%(A%)=0
 A%=A%+1
UNTIL A%>9

M%=1
CLS
PRINT "Who plays first";CHR$(63);
AT 1,2
PRINT "Psion or Human";
DO
 K%=ASC(UPPER$(GET$))
UNTIL K%=%P OR K%=%H OR K%=1
CLS
I%=-(K%=%P)
RETURN K%
